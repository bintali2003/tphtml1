/**
* Checks whether the array of recommended blocks is composed exclusively from
* Entire places (privacy_level >=2)
*
* @param {Array} blocks - the array of recommended blocks
* @returns {boolean|undefined}
*/
function recommendsOnlyEntirePlaces(blocks) {
if(Array.isArray(blocks)) {
return blocks.every(function(targetBlock){
return (targetBlock.b_unit_configuration &&
targetBlock.b_unit_configuration.privacy_level &&
targetBlock.b_unit_configuration.privacy_level >= 2)
})
}
}
/**
* Checks whether the iw-open/fixed-iw-open event will render a
* multi-room recommendation
*
* @param {*} e - the event object related to an iw-open/fixed-iw-open event
* @returns {boolean}
*/
function isMultiRooms(e) {
return getBlocks(e) && getBlocks(e).length > 1
}
/**
* Gets the array of recommended blocks from an iw-open/fixed-iw-open event object
*
* @param {*} e - the event object related to an iw-open/fixed-iw-open event
* @returns {Array|undefined} Array of recommended blocks or undefined
*/
function getBlocks(e) {
var property = e.property;
if( property &&
property.b_availability &&
property.b_availability.b_blocks &&
Array.isArray(property.b_availability.b_blocks) &&
property.b_availability.b_blocks.length > 0
) {
return property.b_availability.b_blocks
}
}
/**
* Checks if iw-open/fixed-iw-open event is linked to the rendering of an iw card
* that is eligible to show the "unit configuration" BH feature
*
* @param {*} e - the event object related to an iw-open/fixed-iw-open event
* @returns {boolean|undefined}
*/
function isEligibleForUnitConfiguration(e) {
return getBlocks(e) && getBlocks(e).length > 0 && recommendsOnlyEntirePlaces(getBlocks(e))
}
/* exp end bhgwe_d_pp_maps_unit_config_on_card */
